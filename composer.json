{
    "name": "mr-vaco/orchid-statuses-plugin",
    "description": "Statuses management",
    "keywords": [
        "laravel",
        "orchid statuses",
        "orchid statuses manager",
        "orchid statuses management"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "mr-vaco/helper-code-for-laravel": "@dev",
        "mr-vaco/orchid-helper-code": "@dev"
    },
    "autoload": {
        "psr-4": {
            "MrVaco\\OrchidStatuses\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrVaco\\OrchidStatuses\\StatusesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
